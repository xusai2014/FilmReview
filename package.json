{
  "name": "filereview",
  "version": "1.0.0",
  "description": "这是一个电影社区项目",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "clean": "sudo rm -rf dist && sudo rm -rf bin",
    "prestart": "npm run clean ",
    "start": "npm-run-all --parallel watch:client watch:server start:service",
    "watch:client": "./node_modules/.bin/webpack --watch",
    "watch:server": "babel src --watch -d dist",
    "start:service": "sudo nodemon ./dist/server/www.js --watch"
  },
  "keywords": [
    "Film",
    "Review",
    "Jerry"
  ],
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-decorators-legacy"
    ]
  },
  "author": "JerryXu",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.22.0",
    "bootstrap": "^3.3.7",
    "bootstrap-loader": "^2.0.0-beta.22",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "react-scripts": "0.8.5",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.1",
    "scss-loader": "0.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "antd": "^3.0.1",
    "bootstrap": "^3.3.7",
    "express": "^4.15.2",
    "jquery": "^3.2.1",
    "jquery.cookie": "^1.4.1",
    "mongoose": "^4.9.0",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0"
  }
}
